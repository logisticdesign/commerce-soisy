{
    "name": "logisticdesign/commerce-soisy",
    "description": "Soisy payment gateway for Craft Commerce",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "commerce",
        "craftcms",
        "soisy",
        "yii2"
    ],
    "support": {
        "docs": "https://github.com/logisticdesign/soisy/blob/master/README.md",
        "issues": "https://github.com/logisticdesign/soisy/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Logistic Design srl",
            "homepage": "https://logisticdesign.it"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.5",
        "craftcms/commerce": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
          "logisticdesign\\soisy\\": "src/"
        }
    },
    "extra": {
        "name": "Soisy for Craft Commerce",
        "handle": "commerce-soisy",
        "developer": "Logistic Design srl",
        "developerUrl": "https://logisticdesign.it",
        "documentationUrl": "https://github.com/logisticdesign/commerce-soisy/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/logisticdesign/commerce-soisy/master/CHANGELOG.md",
        "class": "logisticdesign\\soisy\\Soisy"
    }
}
